#include <stdio.h>

#define N 10
 
  float arr1[5];
  int n;
  int number, i;
  float a[N];
  float b[N];
    

int main(void) {

  printf(" Nombre d'elements des tableaus :\n");

  scanf("%d",&n);


  printf("Affiche les nombres du tableau 1:\n");
  for(i=0;i<n;i++)
  {
	  
	  scanf("%f",a+i);
	}

  printf("Affiche les nombres du tableau 2:\n");
  for(i=0;i<n;i++)
  {
	  
	  scanf("%f",b+i);
	}




  
  number = 0;
    for (i = 0; i < n; i++){
        if (a[i] == b[i])
            number++;
    }
    printf("%d\n", number);
  return 0;
}
